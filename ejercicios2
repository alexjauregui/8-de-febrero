%% Problema 1
syms x a b y c
eqn = 2*x+a;
A = solve(eqn==5, x)

%% Problema 2
syms x a b y c
eqn = x^2+a*x+b;
B = solve(eqn==0, x)

%% Problema 3
syms x a b y c
eqn = 2*exp(x)+3*cos(x);
C = solve(eqn==0, x)

%% Problema 4
syms x a b y c
eqn = 2*x-3*c*y;
eqn2 = c*x+2*y;
D = solve([eqn==5,eqn2==7], [x,y]);
DX=D.x
DY=D.y 

%% Problema 5
syms x a b y c
eqn = 3*x^2-2*x+y;
eqn2 = x*y+x;
E = solve([eqn==7,eqn2==5], [x,y]);
EX=E.x
EY=E.y 
